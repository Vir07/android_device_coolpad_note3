<resources>

    <string name="gesture_double_twist_sensor_name" translatable="false">Coolpad Sensor</string>
    <string name="gesture_double_twist_sensor_vendor" translatable="false">Coolpad</string>

    <bool name="config_voice_over_lte_enabled">true</bool>

    <bool name="tethering_show_help_for_first_using">true</bool>
    <bool name="tethering_show_help_menu">true</bool>

    <!-- When true enable gesture setting. -->
    <bool name="config_gesture_settings_enabled">true</bool>

    <!-- Dashboard number of columns -->
    <integer name="dashboard_num_columns">2</integer>

    <bool name="config_displayDeviceName">true</bool>

    <bool name="config_enableRecoveryUpdater">false</bool>

    <!-- SAR Level for a device -->
    <!-- Must be overlaid by device -->
    <bool name="config_show_sar_enable" translatable="false">true</bool>
    <!-- Maximum Specific Absorption Rate for head -->
    <string name="sar_head_level" translatable="false">0.249</string>
    <!-- Maximum Specific Absorption Rate for body -->
    <string name="sar_body_level" translatable="false">0.245</string>
</resources>
